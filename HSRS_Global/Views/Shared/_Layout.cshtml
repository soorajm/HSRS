        $("#Hsrsgoogleplus").hoverIntent(makeTall, makeShort);
        $("#Hsrslinkedin").hoverIntent(makeTall, makeShort);

        function makeShort() { $(this).animate({ marginRight: "-140px" }, 800, "easeOutBounce"); $(this).css("background-color", "transparent"); }
        function makeTall() { $(this).animate({ marginRight: "10px" }, 800, "easeOutBounce"); $(this).css("background-color", "grey") }
        $("#HsrsSocialFB").click(function () { window.open('https://www.facebook.com/hsrsglobal'); });
