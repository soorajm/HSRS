    <script id="js">
        var strURL = window.location.href
        var strTab = strURL.substring(window.location.href.indexOf("#") + 1);
        $(".Tab_main").focus();
        if (strTab == "faq") {
            $('.second_act').addClass('active in');
            $('.first_act').removeClass('active in');
            $('.third_act').removeClass('active in');
            $('.fourth_act').removeClass('active in');
        }
        if (strTab == "overview") {

            $('.third_act').addClass('active in');
            $('.first_act').removeClass('active in');
            $('.second_act').removeClass('active in');
            $('.fourth_act').removeClass('active in');
        }

        //Tab control

        $('.rgt_side_tab li a.faq, .dropdown-menu li a.faq').on('click', function () {
            $('.second_act').addClass('active in');
            $('.first_act').removeClass('active in');
            $('.third_act').removeClass('active in');
            $('.fourth_act').removeClass('active in');
        });
        $('.rgt_side_tab li a.overview, .dropdown-menu li a.overview').on('click', function () {
            $('.third_act').addClass('active in');
            $('.first_act').removeClass('active in');
            $('.second_act').removeClass('active in');
            $('.fourth_act').removeClass('active in');
        });
        $('.rgt_side_tab li a.include').on('click', function () {
            $('.first_act').addClass('active in');
            $('.third_act').removeClass('active in');
            $('.second_act').removeClass('active in');
            $('.fourth_act').removeClass('active in');
        });
        $('.rgt_side_tab li a.testimonal').on('click', function () {
            $('.fourth_act').addClass('active in');
            $('.first_act').removeClass('active in');
            $('.second_act').removeClass('active in');
            $('.third_act').removeClass('active in');
        });




        //Accordion

        jQuery(function ($) {
            var $active = $('#accordion .panel-collapse.in').prev().addClass('active');
            $active.find('a').append('<span class="plus_minus pull-left"></span>');
            $('#accordion .panel-heading').not($active).find('a').prepend('<span class="plus_minus pull-left"></span>');
            $('#accordion').on('show.bs.collapse', function (e) {
                $('#accordion .panel-heading.active').removeClass('active').find('.glyphicon').toggleClass('glyphicon-plus glyphicon-minus');
                $(e.target).prev().addClass('active').find('.glyphicon').toggleClass('glyphicon-plus glyphicon-minus');
            });
            $('#accordion').on('hide.bs.collapse', function (e) {
                $(e.target).prev().removeClass('active').find('.glyphicon').removeClass('glyphicon-minus').addClass('glyphicon-plus');
            });
        });







        $(document).ready(function () {



            setTimeout(function () {
                $('#hsrs_table').DataTable({
                    initComplete: function () {
                        var api = this.api();

                        api.columns().indexes().flatten().each(function (i) {
                            var column = api.column(i);
                            var e = ["SELECT A COURSE", "SELECT A DATE", "SELECT A LOCATION"];
                            var select = $('<select placeholder="select your beverage"><option value="' + e[i] + '">' + e[i] + '</option></select>')
                                .appendTo($(column.footer()).empty())
                                .on('change', function () {
                                    var val = $.fn.dataTable.util.escapeRegex(
                                        $(this).val()
                                    );

                                    column
                                        .search(val ? '^' + val + '$' : '', true, false)
                                        .draw();
                                });

                            column.data().unique().sort().each(function (d, j) {
                                select.append('<option value="' + d + '">' + d + '</option>')
                            });
                        });
                    }

                }, 10000);


            });


        });




    </script>
    <script>
